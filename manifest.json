{
    "name": "Todoify",
    "short_name": "Todoify",
    "version": "0.2",
    "description": "Replace new tab page with a nice background and my todo list.",
    "browser_action": {
        "default_title": "Todoify"
    },
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "background": {
        "persistent": false,
        "scripts": [ "background.js" ]
    },
    "icons": {
        "128": "big-icon.png",
        "16": "small-icon.png"
    },
    "offline_enabled": true,
    "manifest_version": 2,
    "update_url": "https://clients2.google.com/service/update2/crx"
}
